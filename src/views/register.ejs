<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <link href="../css/styles.css" rel="stylesheet">


    <title>Register</title>
  </head>
  <body>


    <div id="site_wrapper">

      <div id="header_wrap">
        <header>

          <nav role="navigation"> 
            <div id="navigation_container">

              <div class="navbar">
                <div class="logo_container">
                  <img class="logo" src="../images/cc_logo.png" alt="">
                  <span class="logo_text"> Captured Content</span>
                </div>
                  <ul class="nav_list">
                      
                      <li class="nav_item">
                        <div class="nav_item_container">
                          <a href="/"> Home </a>
                        </div>
                      </li>

                      <li class="nav_item">
                        <div class="nav_item_container">
                          <a href="#"> Account </a>
                        </div>
                      </li>

                      <li class="nav_item">
                        <div class="nav_item_container">
                          <a href="#"> Login </a>
                        </div>
                      </li>

                      <li class="nav_item">
                        <div class="nav_item_container">
                          <a href="#"> Register </a>
                        </div>
                      </li>
                  </ul>
              </div>
            </div>
          </nav>

        </header>
      </div>


      <div class="create_account_container">

        <div id="Login">
          <div class="form_styling">

            <h2>Create an account</h2>
            <form name="signup" action="/auth/signUp" method="POST">
              First name: <input name="fName" id="fName" > <br> 
              Last Name: <input name="lName" id="lName" > <br> 
              Username: <input name="username" id="username" > <br> 
              Password: <input name="password" id="password" type="password" > <br> 
           <input type="submit" value="Register" class=""></input>
           </form>
          </div>
        </div>
      </div>

    </div>



    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <script
    src="https://code.jquery.com/jquery-3.6.0.min.js"
    integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
    crossorigin="anonymous"></script>

  </body>
</html>